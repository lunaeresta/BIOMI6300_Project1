placebo_data <- data.frame(matrix(ncol=2, nrow=93))
names(placebo_data) <- c("group", "test_result")
# Simulating test results for B12 and Placebo groups
set.seed(123)
b12_simulation <- rnorm(91, mean=23.9, sd=6.677)
placebo_simulation <- rnorm(93, mean=24.6, sd=6.677)
# Assigning test results into the B12 and Placebo tables
b12_data$group <- "b12"
b12_data$test_result <- b12_simulation
placebo_data$group <- "placebo"
placebo_data$test_result <- placebo_simulation
# Combining the B12 and Placebo tables into one combined table for the experiment
simulated_data <- rbind(b12_data, placebo_data)
View(simulated_data)
